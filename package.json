{
  "name": "node",
  "version": "1.0.0",
  "description": "Starter kit for express backend",
  "scripts": {
    "start": "nodemon src/index.ts",
    "build": "rm -rf build && mkdir build && esbuild src/index.ts --bundle --sourcemap --minify --platform=node --target=node15.6 --outfile=./build/server.js",
    "test": "jest",
    "lint": "eslint ./src"
  },
  "repository": {
    "type": "git"
  },
  "keywords": [
    "express",
    "backend",
    "nodejs",
    "typescript"
  ],
  "author": "Ludovic Cluber",
  "license": "MIT",
  "dependencies": {
    "@dwtechs/checkhard": "2.17.1",
    "body-parser": "1.19.0",
    "cookie-parser": "1.4.6",
    "cors": "2.8.5",
    "express": "4.17.1",
    "helmet": "4.6.0",
    "morgan": "1.10.0",
    "multer": "1.4.4",
    "passport": "0.5.0"
  },
  "devDependencies": {
    "@types/body-parser": "1.19.2",
    "@types/cookie-parser": "1.4.2",
    "@types/cors": "2.8.12",
    "@types/express": "4.17.13",
    "@types/jest": "27.0.3",
    "@types/morgan": "1.9.3",
    "@types/node": "16.11.12",
    "@typescript-eslint/eslint-plugin": "5.6.0",
    "@typescript-eslint/parser": "5.6.0",
    "esbuild": "0.14.5",
    "eslint": "8.4.1",
    "jest": "27.4.3",
    "nodemon": "2.0.15",
    "ts-jest": "27.1.2",
    "ts-node": "10.4.0",
    "typescript": "4.5.2"
  }
}
